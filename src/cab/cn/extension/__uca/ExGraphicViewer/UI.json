        "addon": {
            "toolbar": {
                "container": "web-g6-viewer-toolbar",
                "commands": {
                    "view": [
                        [
                            "创建 ( Cmd + N )",
                            "重置 ( Cmd + R )",
                            "打印 ( Cmd + P )",
                            "放大 ( Cmd + + )",
                            "缩小 ( Cmd + - )"
                        ]
                    ],
                    "disabled": [
                        "plus",
                        "print",
                        "group",
                        "tpl"
                    ]
                },
                "css": {
                    "height": 36
                }
